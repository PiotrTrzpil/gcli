{
  "name": "@piotrtrzpil/gcli",
  "version": "1.0.0",
  "description": "gcli",
  "license": "MIT",
  "repository": "https://github.com/PiotrTrzpil/gcli",
  "author": {
    "name": "PT",
    "email": "",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "gtray": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5 --print-width 120",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "compile": "tsc --pretty",
    "test": "mocha",
    "watch": "tsc --watch",
    "codegen": "source github.env && graphql-codegen --config codegen.yml",
    "watch:test": "npm run pretest && mocha-typescript-watch lib/**/*.spec.js --timeout 5000"
  },
  "dependencies": {
    "@types/chai": "^4.2.3",
    "apollo-boost": "^0.1.28",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "chai": "^4.2.0",
    "graphql": "^0.13.2",
    "graphql-config": "latest",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^3.1.1",
    "js-yaml": "^3.13.1",
    "json2typescript": "^1.2.3",
    "lodash": "^4.17.21",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.13",
    "sywac": "^1.2.2",
    "typescript-logging": "^0.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/schema-ast": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@types/graphql": "^0.13.4",
    "@types/jest": "^22.2.3",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.139",
    "@types/node": "^8.10.54",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "coveralls": "^2.0.0",
    "eslint": "^6.2.2",
    "mocha": "^5.0.1",
    "mocha-typescript": "^1.1.17",
    "prettier": "^1.18.2",
    "rimraf": "^2.7.1",
    "ts-node": "^3.3.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
