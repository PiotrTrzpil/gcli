{
  "name": "@piotrtrzpil/gcli",
  "version": "1.0.0",
  "description": "gcli",
  "license": "MIT",
  "repository": "https://github.com/PiotrTrzpil/gcli",
  "author": {
    "name": "PT",
    "email": "",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "gtray": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5 --print-width 120",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "compile": "tsc --pretty",
    "build": "tsc --pretty",
    "pretest": "npm run clean && tsc",
    "test": "mocha lib/**/*.spec.js --timeout 5000",
    "watch": "tsc --watch",
    "watch:test": "npm run pretest && mocha-typescript-watch lib/**/*.spec.js --timeout 5000"
  },
  "dependencies": {
    "@types/chai": "^4.1.6",
    "apollo-boost": "^0.1.4",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.4",
    "chai": "^4.2.0",
    "commander": "^2.15.1",
    "graphql": "^0.13.2",
    "graphql-config": "latest",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "js-yaml": "latest",
    "json2typescript": "latest",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "reflect-metadata": "^0.1.12",
    "source-map-support": "^0.5.6",
    "sywac": "^1.2.1",
    "typescript-logging": "^0.5.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.0",
    "@types/jest": "^22.2.3",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.10.30",
    "coveralls": "^2.0.0",
    "jest": "^22.0.4",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.3.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "testRegex": "__tests__/.*\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
